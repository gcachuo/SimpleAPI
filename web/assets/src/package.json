  "devDependencies": {
    "@types/bootstrap": "^5.0.0",
    "@types/datatables.net": "^1.10.19",
    "@types/datatables.net-buttons": "^1.4.3",
    "@types/jquery": "^3.5.1",
    "@types/node": "^14.10.1",
    "css-loader": "^4.3.0",
    "cypress": "^6.7.1",
    "expose-loader": "^1.0.0",
    "file-loader": "^6.1.0",
    "husky": "^5.1.3",
    "imports-loader": "^1.2.0",
    "jszip": "^3.5.0",
    "pdfmake": "^0.1.68",
    "sass": "^1.26.11",
